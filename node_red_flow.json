    {
        "id": "febb79b3a96a4220",
        "type": "function",
        "z": "61de64c2602e1149",
        "g": "6ab856272578b1e2",
        "name": "function 10",
        "func": "var currentTemp = msg.payload.Temperature;\n\n// Get previous temperature from context (or use current if no previous exists)\nvar prevTemp = context.get('prevTemp') || currentTemp;\n\n// Calculate delta (difference between current and previous)\nvar delta = currentTemp - prevTemp;\n\n// Round to 2 decimal places\ndelta = Math.round(delta * 100) / 100;\n\n// Store current temperature for next time\ncontext.set('prevTemp', currentTemp);\n\n// Set the delta as the payload\nmsg.payload = delta;\n\nreturn msg;",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
